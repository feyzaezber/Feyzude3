@model IEnumerable<Feyzude3.Models.Randevu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RandevuName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RandevuSaat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RandevuTelefon)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RandevuName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RandevuSaat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RandevuTelefon)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RandevuId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RandevuId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RandevuId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
